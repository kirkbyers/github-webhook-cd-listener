{
  "name": "@kbye/ghcd",
  "version": "1.0.2",
  "description": "An Express CLI tool for setting up continuous delivery with github webhooks",
  "main": "index.js",
  "scripts": {
    "dev": "tsc -w",
    "build": "tsc && echo '#!/usr/bin/env node' | cat - index.js > temp && mv temp index.js",
    "start": "tsc && node --harmony ./index.js",
    "test": "rimraf ./dist && tsc && concurrently 'tsc -w' 'jest --watch ./dist'"
  },
  "bin": {
    "ghcd": "index.js"
  },
  "keywords": [
    "github",
    "continuous",
    "delivery"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "commander": "^2.13.0",
    "express": "^4.16.2"
  },
  "devDependencies": {
    "@types/express": "^4.11.0",
    "@types/jest": "^21.1.8",
    "@types/node": "^9.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "jest": "^21.2.1",
    "nodemon": "^1.12.1",
    "rimraf": "^2.6.2",
    "concurrently": "^3.5.1"
  }
}